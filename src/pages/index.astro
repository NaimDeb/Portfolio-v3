---
import Header from "@/components/Header.astro";
import Hero from "@/components/Hero.astro";
import Layout from "@/layouts/Layout.astro";
import Stack from "@/components/Stack.astro";
import Projects from "@/components/Projects.astro";
import About from "@/components/About.astro";
import Footer from "@/components/Footer.astro";
import CVButton from "@/components/CVButton.astro";
---

<Layout title="Free Astro Portfolio - LiteFolio">
  <main
    class="relative w-full flex flex-col justify-start items-center bg-pattern dark:bg-dark-pattern bg-blend-multiply"
  >
    <Header />
    <Hero />
    <Stack />
    <Projects />
    <About />
    <Footer />
    <CVButton />
  </main>
</Layout>


<style>
  .bg-blend-multiply{
    background-blend-mode: multiply;

  }
</style>